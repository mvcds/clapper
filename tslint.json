{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "semicolon": [
      true,
      "never",
      "ignore-interfaces"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template",
      "jsx-double"
    ],
    "ordered-imports": false
  },
  "rulesDirectory": []
}
